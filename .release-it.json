{
      "git": {
        "requireBranch": "master",
         "tagName": "${version}",
         "commitMessage": "CGY: release v${version}"
      },
      "hooks": {
        "before:init": ["git pull"],
    	  "after:bump": "npm auto-changelog -p"
      },
      "github": {
        "release": true
      },
      "npm": {
        "publish": true
      }
}
